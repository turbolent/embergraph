<?xml version="1.0"?>
<sparql
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xs="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://www.w3.org/2005/sparql-results#" >
  <head>
    <variable name="res"/>
    <variable name="label"/>
  </head>
  <results>
     <result>
      <binding name="res">
         <uri>http://example.com/Apple</uri>
      </binding>
      <binding name="label">
         <literal>Apple (RDF)</literal>
      </binding>
     </result>
     <result>
      <binding name="res">
         <uri>http://example.com/SAP</uri>
      </binding>
      <binding name="label">
         <literal>SAP (RDF)</literal>
      </binding>
     </result>     
  </results>
</sparql>