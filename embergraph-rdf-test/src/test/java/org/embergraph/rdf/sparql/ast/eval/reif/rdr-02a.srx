<?xml version="1.0"?>
<sparql
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:xs="http://www.w3.org/2001/XMLSchema#"
     xmlns="http://www.w3.org/2005/sparql-results#" >
   <head>
    <variable name="who"/>
    <variable name="src"/>
    <variable name="created"/>
   </head>
   <results>
     <result>
        <binding name="who">
            <uri>http://example.com/SAP</uri>
        </binding>
        <binding name="src">
            <uri>http://example.com/news/us-sybase</uri>
        </binding>
        <binding name="created">
            <literal datatype="http://www.w3.org/2001/XMLSchema#dateTime">2011-04-05T12:00:00Z</literal>
        </binding>
     </result>
   </results>
</sparql>