PREFIX geo: <http://www.embergraph.org/rdf/geospatial#>

SELECT * WHERE {
  SERVICE geo:search {
    ?res geo:searchDatatype <http://my.custom.datatype/x-y-z> .
    ?res geo:predicate <http://p> .
    ?res geo:customFields "y#x#z" .
    ?res geo:customFieldsLowerBounds "0.4#0#1" .
    ?res geo:customFieldsUpperBounds "1#1#1" .
  }
}