PREFIX geo: <http://www.embergraph.org/rdf/geospatial#>

SELECT * WHERE {
  SERVICE geo:search {
    ?res geo:search "inCircle" .
    ?res geo:predicate <http://p> .
    ?res geo:spatialCircleCenter "4#4" .
    ?res geo:spatialCircleRadius "112" . #km
    ?res geo:timeStart "5" .
    ?res geo:timeEnd "5" .
  }
} 