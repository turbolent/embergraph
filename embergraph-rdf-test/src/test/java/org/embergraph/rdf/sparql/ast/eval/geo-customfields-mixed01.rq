PREFIX geo: <http://www.embergraph.org/rdf/geospatial#>

SELECT * WHERE {
  {
    SERVICE geo:search {
      ?res geo:searchDatatype <http://my.custom.datatype/time-x-y-z> .
      ?res geo:predicate <http://p> .
      ?res geo:customFields "x#y#z" .
      ?res geo:customFieldsLowerBounds "0#0.4#1" .
      ?res geo:customFieldsUpperBounds "1#1#1" .
      ?res geo:timeStart "1" .
      ?res geo:timeEnd "2" .    
    }
  } UNION {
    SERVICE geo:search {
      ?res geo:searchDatatype <http://my.custom.datatype/x-y-z> .
      ?res geo:predicate <http://p> .
      ?res geo:customFields "x#y#z" .
      ?res geo:customFieldsLowerBounds "0#0.4#1" .
      ?res geo:customFieldsUpperBounds "1#1#1" .
    }
  } UNION {
    ?res geo:search "inCircle" .
    ?res geo:searchDatatype <http://my.custom.datatype/x-y-z-lat-lon-time> .
    ?res geo:predicate <http://p> .
    ?res geo:spatialCircleCenter "1#1" .
    ?res geo:spatialCircleRadius "1" . # 1KM    
    ?res geo:customFields "x#y#z" .
    ?res geo:customFieldsLowerBounds "0#0.4#1" .
    ?res geo:customFieldsUpperBounds "1#1#1" .
    ?res geo:timeStart "1" .
    ?res geo:timeEnd "2" .  
  }
}