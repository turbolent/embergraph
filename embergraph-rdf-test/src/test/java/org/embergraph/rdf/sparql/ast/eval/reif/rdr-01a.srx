            <literal>Bob</literal>
        </binding>
        <binding name="src">
            <uri>http://reasoner.example.com/engines#engine_1</uri>
        </binding>
        <binding name="conf">
            <literal datatype="http://www.w3.org/2001/XMLSchema#decimal">0.9835</literal>
        </binding>
     </result>
   </results>
</sparql>