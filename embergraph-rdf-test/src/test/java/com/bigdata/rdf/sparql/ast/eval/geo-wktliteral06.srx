<?xml version="1.0"?>
<sparql
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xs="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://www.w3.org/2005/sparql-results#" >
  <head>
    <variable name="res"/>
    <variable name="location"/>
    <variable name="lat"/>
    <variable name="lon"/>
    <variable name="lit"/>
    <variable name="circleCenter"/>    
  </head>
  
  <results>
     <result>
      <binding name="res"><uri>http://s.1#1</uri></binding>
      <binding name="lat"><literal datatype="http://www.w3.org/2001/XMLSchema#double">1.0</literal></binding>
      <binding name="lon"><literal datatype="http://www.w3.org/2001/XMLSchema#double">1.0</literal></binding>
      <binding name="location"><literal datatype="http://www.opengis.net/ont/geosparql#wktLiteral">Point(1.0,1.0)</literal></binding>
      <binding name="lit"><literal datatype="http://www.opengis.net/ont/geosparql#wktLiteral">Point(1.0,1.0)</literal></binding>
      <binding name="circleCenter"><literal datatype="http://www.opengis.net/ont/geosparql#wktLiteral">Point(1.0,1.0)</literal></binding>
     </result>
  </results>
</sparql>