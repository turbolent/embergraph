PREFIX geo: <http://www.bigdata.com/rdf/geospatial#>

SELECT * WHERE {
  SERVICE geo:search {
    ?res geo:search "inCircle" .
    ?res geo:predicate <http://p> .
    ?res geo:spatialCircleCenter "1#9" .
    ?res geo:spatialCircleRadius "240" . 
    ?res geo:timeStart "5" .
    ?res geo:timeEnd "5" .
  }
} 