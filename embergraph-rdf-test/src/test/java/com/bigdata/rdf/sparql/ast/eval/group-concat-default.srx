<?xml version="1.0"?>
<sparql
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:xs="http://www.w3.org/2001/XMLSchema#"
    xmlns="http://www.w3.org/2005/sparql-results#" >
  <head>
    <variable name="s"/>
    <variable name="phrase"/>
  </head>
  <results>
    <result>
      <binding name="s">
      	<uri>http://example.org/s1</uri>
      </binding>
      <binding name="phrase">
      	<literal>a;in;park;the;walk</literal>
      </binding>
    </result>
    <result>
      <binding name="s">
      	<uri>http://example.org/s2</uri>
      </binding>
      <binding name="phrase">
      	<literal>4;hi;now</literal>
      </binding>
    </result>
  </results>
</sparql>
